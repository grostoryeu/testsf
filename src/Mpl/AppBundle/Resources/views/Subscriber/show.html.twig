<h1>Subscriber</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Ip</th>
            <td>{{ entity.ip }}</td>
        </tr>
        <tr>
            <th>Date_created</th>
            <td>{{ entity.datecreated|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Date_modified</th>
            <td>{{ entity.datemodified|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Genres</th>
            <td>{{ entity.genres|join(',') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('utilisateurs') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('utilisateurs_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('utilisateurs_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
